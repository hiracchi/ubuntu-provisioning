Host *
  ForwardAgent yes
  Compression yes
  CompressionLevel 9
  ForwardX11 yes
  ForwardX11Trusted yes
  ServerAliveInterval 60
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  IdentityFile ./ssh/pdfadmin

Host vagrant
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ./ssh/vagrant_private_key
  IdentitiesOnly yes
  LogLevel FATAL    
    

